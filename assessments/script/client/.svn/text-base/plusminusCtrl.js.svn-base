	function minus(txtField, dec){
		var val=txtField.value;
		val++; val--;
		if(val>0)
			if(dec){
				val-=dec;
				if(val < 0) val=0;
			}else
				val--;
				
		txtField.value=val;
	}
	
	function plus(txtField, inc){
		var val=txtField.value;
		val++; val--;
		if(inc)
			val+=inc;
		else
			val++;
			
		txtField.value=val;
	}
